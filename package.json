{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "A wrapper to work with the Spotify web API",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafamaxber/spotify-wrapper.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Rafael Bernardo <rafamaxber@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willianjusten/js-tdd-course/issues"
  },
  "homepage": "https://github.com/willianjusten/js-tdd-course#readme",
  "devDependencies": {
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "node-fetch": "1.6.3",
    "nyc": "^10.2.0",
    "sinon": "2.2.0",
    "sinon-chai": "2.10.0",
    "sinon-stub-promise": "4.0.0"
  }
}
